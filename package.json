{
  "scripts": {
    "build": "npx rimraf ./dist && swc ./src -d dist",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch ./src -e js,jsx,ts,tsx --exec ts-node ./src/index.ts",
    "clear": "npx rimraf ./dist"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.99",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^2.0.22",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "cross-env": "^7.0.3"
  }
}